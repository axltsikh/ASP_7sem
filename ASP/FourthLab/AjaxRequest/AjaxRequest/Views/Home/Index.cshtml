<input type="number" id="x">
<br />
<br />
<input type="number" id="y">
<br />
<br />
<input type="button" id="addButton" value="AddS result">
<br />
<div id="result"></div>

<script src="~/Scripts/jquery-3.4.1.js" type="text/javascript">
</script>
<script src="~/Scripts/jquery.unobtrusive-ajax.js" type="text/javascript">
</script>

<script type="text/javascript">
    $(document).ready(function () {
        $("#addButton").click(function () {
            xValue = document.getElementById("x").value;
            yValue = document.getElementById("y").value;
            var payload = {
                x: xValue,
                y: yValue
            };
            
            $.ajax({
                url: 'https://localhost:44329/Simplex.asmx/AddS',
                method: 'POST',
                contentType: "application/json; charset=utf-8",
                data: JSON.stringify(payload),
                dataType:'json',
                success: function (data) {
                    $('#result').html("Result: " + data.d);
                }
            });
        });
    });
</script>